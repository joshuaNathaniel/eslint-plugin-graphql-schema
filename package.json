{
  "name": "eslint-plugin-graphql-schema",
  "version": "1.0.0",
  "description": "ESLint Plugin for GraphQL Schema",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Joshua Nathaniel Miller",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuaNathaniel/eslint-plugin-graphql-schema.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "acceptance": "cd acceptance/ && npm install && npm run reset && npm run start && npm run reset && cd ..",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest tests/*"
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "graphql": "15.1.0",
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.3",
    "@babel/core": "7.17.5",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/register": "7.17.0",
    "babel-plugin-istanbul": "6.1.1",
    "babel-polyfill": "6.26.0",
    "cross-env": "7.0.3",
    "eslint": "8.9.0",
    "jest": "26.6.3"
  },
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT"
}
