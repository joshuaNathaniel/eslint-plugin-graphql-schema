{
  "name": "eslint-plugin-graphql-schema",
  "version": "0.3.5",
  "description": "ESLint Plugin for GraphQL Schema",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Joshua Nathaniel Miller",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuaNathaniel/eslint-plugin-graphql-schema.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "acceptance": "cd acceptance/ && npm install && npm run reset && npm run start && npm run reset && cd ..",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest tests/*"
  },
  "dependencies": {
    "@babel/runtime": "7.10.3",
    "graphql": "15.1.0",
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/plugin-transform-runtime": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/register": "7.10.5",
    "babel-plugin-istanbul": "6.0.0",
    "babel-polyfill": "6.26.0",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "jest": "26.4.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "license": "MIT"
}
