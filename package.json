    "@babel/register": "7.10.5",
    "babel-plugin-istanbul": "6.0.0",
    "babel-polyfill": "6.26.0",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
